<p>Name: <%= @gym_class.name %></p>
<p>Start Time: <%= @gym_class.start_time %></p>
<p>Maximum Capacity: <%= @gym_class.max_capacity %></p>
<p>Attendees:</p>
<ul>
<% for booking in @bookings %>
  <li><%= booking.member.name %></li>
</ul>

<% end %>
<p><a href="/classes/<%= @gym_class.id %>/edit">Edit Class Details</a></p>
<form action="/classes/<%= @gym_class.id %>/delete" method="post">
  <input type="submit" value="Delete Class"/>

</form>
